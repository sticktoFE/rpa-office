QWidget {
    border-radius: 16px;
}

/*所有按钮*/
QPushButton {
    max-width: 80px;
    max-height: 24px;
    min-height: 24px;
    border-radius: 6px;
    background-color: rgb(244, 244, 244);
    border: 1px solid rgb(167, 167, 167);
}

QPushButton:hover {
    background-color: rgb(190, 231, 253);
}

QPushButton:pressed {
    background-color: rgb(244, 244, 244);
}

QComboBox {
    font: 13px;
    /* max-width: 80px; */
    max-height: 20px;
    min-height: 20px;
    border: 1px solid rgb(167, 167, 167);
    border-radius: 3px;
    margin: 3px
}

QComboBox QAbstractItemView::item {
    min-height: 12px;
    min-width: 80px;
}

QMenu {
    /* 半透明效果 */
    background-color: rgba(0, 101, 105, 230);
    border: none;
    border-radius: 5px;
}

QMenu::item {
    border-radius: 5px;
    /* 这个距离很麻烦需要根据菜单的长度和图标等因素微调 */
    padding: 8px 48px 8px 36px;
    /* 36px是文字距离左侧距离*/
    background-color: transparent;
    color: aliceblue
        /*字体颜色*/
}

/* 鼠标悬停和按下效果 */
QMenu::item:selected {
    border-radius: 0px;
    /* 半透明效果 */
    background-color: cornflowerblue;
}

/* 禁用效果 */
QMenu::item:disabled {
    background-color: transparent;
}

/* 图标距离左侧距离 */
QMenu::icon {
    left: 15px;
}

/* 分割线效果 */
QMenu::separator {
    height: 1px;
    background-color: rgb(0, 101, 105);
}

/*鼠标悬停颜色*/
HistoryPanel::item:hover {
    background: rgb(52, 52, 52);
}

/* QScrollArea ------------------------------------------------------------

--------------------------------------------------------------------------- */
/* 滚动条区域 */
QAbstractScrollArea {
    background-color: transparent;
    border: 0px solid #455364;
    border-radius: 10px;
    /* fix #159 */
    padding: 2px;
    /* remove min-height to fix #244 */
    color: #E0E1E3;
}

QAbstractScrollArea:disabled {
    color: #9DA9B5;
}

QScrollArea QWidget QWidget:disabled {
    background-color: #19232D;
    border-radius: 12px;
}

/* //首先是设置主体 */
QScrollBar:vertical {
    background-color: rgb(255， 255， 255， 100);
    /* 左分割线   */
    border: 0px solid #572c2c;
    width: 12px;
    margin: 13 0 13 0px;
}

/* //为滚动条上面的箭头区域 */
QScrollBar::sub-line:vertical {
    subcontrol-origin: margin;
    border: 0px solid red;
    height: 13px
}

QScrollBar::up-arrow:vertical {
    border-image: url(:/icon/up.png) center no-repeat;
    subcontrol-origin: margin;
    background-color: blue;
    height: 13px
}

/* 表示已滑过的槽部分 */
QScrollBar::sub-page:vertical {
    background-color: rgb(112, 115, 119);
}

/* //滑块 */
QScrollBar::handle:vertical {
    /* 滑块颜色  */
    min-height: 20px;
    background-color: rgb(134, 79, 134);
    width: 12px;
}

/* 鼠标触及滑块样式   */
QScrollBar::handle:vertical:hover {
    background: #370f81;
}

/* 表示未滑过的槽部分 */
QScrollBar::add-page:vertical {
    background-color: rgb(0, 101, 105);
}

/* //为滚动条下面的箭头区域 */
QScrollBar::down-arrow:vertical {
    background-color: yellow;
    border-image: url(:/icon/down.png) center no-repeat;
}

QScrollBar::add-line:vertical {
    subcontrol-origin: margin;
    border: 0px solid rgb(230, 144, 15);
    height: 13px;
}

QGroupBox {
    border: 0px solid rgb(17, 0, 255);
    border-radius: 1px;
}

